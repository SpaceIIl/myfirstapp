<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CardViewStyle">
        <item name="cardElevation">1dp</item>
        <item name="strokeWidth">0dp</item>
        <item name="cardBackgroundColor">?attr/colorPrimaryContainer</item>
    </style>
    <style name="TextAppearance.MyApplicationA.DisplayLarge" parent="TextAppearance.Material3.BodyLarge">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="TextAppearance.MyApplicationA.DisplayMedium" parent="TextAppearance.Material3.BodyMedium">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
    <style name="TextAppearance.MyApplicationA.DisplaySmall" parent="TextAppearance.Material3.BodySmall">
        <item name="fontFamily">@font/roboto_regular</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">0</item>
    </style>
</resources>